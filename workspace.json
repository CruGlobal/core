{
  "version": 2,
  "projects": {
    "api-journeys": {
      "root": "apps/api-journeys",
      "sourceRoot": "apps/api-journeys/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api-journeys",
            "main": "apps/api-journeys/src/main.ts",
            "tsConfig": "apps/api-journeys/tsconfig.app.json"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api-journeys/src/environments/environment.ts",
                  "with": "apps/api-journeys/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api-journeys:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api-journeys/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api-journeys"],
          "options": {
            "jestConfig": "apps/api-journeys/jest.config.js",
            "passWithNoTests": true
          }
        },
        "generate": {
          "executor": "@nx-tools/nx-prisma:generate",
          "options": {
            "schema": "apps/api-journeys/prisma/schema.prisma"
          }
        },
        "migrations": {
          "executor": "@nx-tools/nx-prisma:migrations",
          "options": {
            "schema": "apps/api-journeys/prisma/schema.prisma"
          }
        },
        "codegen": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "npx graphql-codegen --config apps/api-journeys/codegen.yml"
              }
            ]
          }
        }
      }
    },
    "multiplechoice": {
      "root": "libs/multiplechoice",
      "sourceRoot": "libs/multiplechoice/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/multiplechoice/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/multiplechoice"],
          "options": {
            "jestConfig": "libs/multiplechoice/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/multiplechoice/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/multiplechoice",
            "config": {
              "configFolder": "libs/multiplechoice/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/express"
  },
  "defaultProject": "api-journeys",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  }
}
