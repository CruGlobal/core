{
  "version": 2,
  "projects": {
    "api-journeys": {
      "root": "apps/api-journeys",
      "sourceRoot": "apps/api-journeys/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api-journeys",
            "main": "apps/api-journeys/src/main.ts",
            "tsConfig": "apps/api-journeys/tsconfig.app.json"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api-journeys/src/environments/environment.ts",
                  "with": "apps/api-journeys/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api-journeys:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api-journeys/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api-journeys"],
          "options": {
            "jestConfig": "apps/api-journeys/jest.config.js",
            "passWithNoTests": true
          }
        },
        "generate": {
          "executor": "@nx-tools/nx-prisma:generate",
          "options": {
            "schema": "apps/api-journeys/prisma/schema.prisma"
          }
        },
        "migrations": {
          "executor": "@nx-tools/nx-prisma:migrations",
          "options": {
            "schema": "apps/api-journeys/prisma/schema.prisma"
          }
        },
        "codegen": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "npx graphql-codegen --config apps/api-journeys/codegen.yml"
              }
            ]
          }
        }
      }
    },
    "tools": {
      "root": "libs/tools",
      "sourceRoot": "libs/tools/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/tools/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tools"],
          "options": {
            "jestConfig": "libs/tools/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/tools",
            "tsConfig": "libs/tools/tsconfig.lib.json",
            "packageJson": "libs/tools/package.json",
            "main": "libs/tools/src/index.ts",
            "assets": [
              "libs/tools/*.md",
              {
                "input": "./libs/tools/src",
                "glob": "**/!(*.ts)",
                "output": "./src"
              },
              {
                "input": "./libs/tools/src",
                "glob": "**/*.d.ts",
                "output": "./src"
              },
              {
                "input": "./libs/tools",
                "glob": "generators.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "tools-e2e": {
      "root": "apps/tools-e2e",
      "projectType": "application",
      "sourceRoot": "apps/tools-e2e/src",
      "targets": {
        "e2e": {
          "executor": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "tools:build",
            "jestConfig": "apps/tools-e2e/jest.config.js"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/express"
  },
  "defaultProject": "api-journeys"
}
